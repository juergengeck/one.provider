  "scripts": {
    "build": "node ./build.js",
    "build:src": "tsc --build src/tsconfig.json",
    "build:test": "tsc --build test/tsconfig.json",
    "doc": "npx --no-install typedoc",
    "prepare": "node ./build.js",
    "start": "node lib/model/index.js",
    "test": "npm run build:test && npx --no-install mocha --exit 'test/**/*-test.js'",
    "testcov": "npm run build:test && npx --no-install nyc npx --no-install mocha --exit 'test/**/**-test.js'",
    "start-comm-server": "node comm_server.bundle.js -h localhost -p 8000 -l",
    "start-state-machine": "node lib/tools/StateMachine",
    "bundle": "npx --no-install webpack",
    "cpall": "node copy-refinio-deps.mjs",
    "cpcore": "node copy-refinio-deps.mjs core"
  },
  "mocha": {
    "require": [
      "source-map-support/register"
    ],
    "enable-source-maps": true
  },
  "dependencies": {
    "@refinio/one.core": "file:../one.core",
